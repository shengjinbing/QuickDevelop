<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.modesty.quickdevelop.ui.activitys.FunctionActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="delay"
            android:text="delay操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="doo"
            android:text="do操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="onErrorReturn"
            android:text="onErrorReturn操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="onErrorResumeNext"
            android:text="onErrorResumeNext/若需拦截Exception请用onExceptionResumeNext操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="retry"
            android:text="retry操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="retryWhen"
            android:text="retryWhen操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="repeat"
            android:text="repeat操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="repeatWhen"
            android:text="repeatWhen操作符" />
    </LinearLayout>
</ScrollView>
