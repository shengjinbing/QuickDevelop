<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.modesty.quickdevelop.ui.activitys.RxjavaConcatActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="concat"
            android:text="concat/concatArray组合操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="merge"
            android:text="merge/mergeArray组合操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="concatDelayError"
            android:text="concatDelayError/mergeDelayError组合操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="zip"
            android:text="zip合并操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="combineLatest"
            android:text="combineLatest合并操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="reduce"
            android:text="reduce合并操作符" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="collect"
            android:text="collect合并操作符" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="startWith"
            android:text="startWith/startWithArray合并操作符" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#cf9292"
            android:gravity="center"
            android:onClick="count"
            android:text="count合并操作符" />
    </LinearLayout>
</ScrollView>
